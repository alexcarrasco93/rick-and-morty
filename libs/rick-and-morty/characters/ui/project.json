{
  "projectType": "library",
  "root": "libs/rick-and-morty/characters/ui",
  "sourceRoot": "libs/rick-and-morty/characters/ui/src",
  "prefix": "workspace",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/rick-and-morty/characters/ui"],
